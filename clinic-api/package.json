{
  "name": "clinic-api",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "tsx scripts/start-dev.ts",
    "dev:simple": "nodemon --exec tsx src/server.ts",
    "dev:local": "./scripts/start-local.sh",
    "deploy": "npm run build && ./deploy.sh",
    "db:fresh": "cd db && ./manage_db.sh fresh",
    "db:schema": "cd db && ./manage_db.sh schema",
    "db:seed": "cd db && ./manage_db.sh seed",
    "db:check": "cd db && ./manage_db.sh check",
    "db:reset": "cd db && ./manage_db.sh reset",
    "db:backup": "cd db && ./manage_db.sh backup",
    "db:cloud:start": "gcloud sql instances patch clinic-db --activation-policy=ALWAYS",
    "db:cloud:stop": "gcloud sql instances patch clinic-db --activation-policy=NEVER",
    "db:cloud:status": "gcloud sql instances describe clinic-db --format=\"value(state)\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "firebase-admin": "^13.1.0",
    "googleapis": "^144.0.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.3",
    "pg-pool": "^3.7.1",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.13.4",
    "@types/pg": "^8.11.11",
    "ngrok": "^5.0.0-beta.2",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
